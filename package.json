{
  "name": "svelte-router",
  "version": "2.0.0-beta.0",
  "description": "A simple router for Svelte",
  "main": "lib/svelte-router.js",
  "author": "Jikkai Xiao <sonne@asaki.me>",
  "license": "MIT",
  "scripts": {
    "prebuild:js": "rimraf ./lib",
    "build:js": "cross-env NODE_ENV=production APP_ENV=js rollup -c",
